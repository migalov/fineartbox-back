      "repeatable": true
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "cardsSection": {
      "type": "component",
      "component": "grids.cards-list",
      "repeatable": true
    },
    "SEO": {
      "type": "component",
      "component": "grids.seo-block",
      "repeatable": false
