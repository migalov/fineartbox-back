      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "numTabs": {
      "type": "integer"
    },
    "linksConstructions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::filter-construction.filter-construction"
    },
    "filterConstructions": {
      "type": "component",
      "component": "filter.option",
      "repeatable": true
