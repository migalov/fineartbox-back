{
  "kind": "collectionType",
  "collectionName": "package_types",
  "info": {
    "singularName": "package-type",
    "pluralName": "package-types",
    "displayName": "Package Type"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "packages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::package.package",
      "mappedBy": "package_types"
    },
    "descriptionTypePackage": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "descriptionProcess": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "slide": {
      "type": "component",
      "component": "slider.slide",
      "repeatable": false
    },
    "tag": {
      "type": "string",
      "unique": true
    },
    "SEO": {
      "type": "component",
      "component": "grids.seo-block",
      "repeatable": false
    },
    "drawing": {
      "type": "string"
    },
    "constructionTypes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::construction-type.construction-type",
      "mappedBy": "packageType"
    }
  }
}
